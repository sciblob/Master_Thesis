% Schriftentest Anfang
% \TUBAFSchriftanpassung[Class/Package]{<Name ohne TUBAF>}{cmss/cmr}
\newcommand{\TUBAFSchriftanpassung}[3][Class]{%
	\ifluatex
		\csname#1WarningNoLine\endcsname{\csname TUBAF#2@name\endcsname}{Kuemmern Sie sich bitte selbst\MessageBreak um die Fontangelegenheiten}%
	   \else
		\ifxetex
			\csname#1WarningNoLine\endcsname{\csname TUBAF#2@name\endcsname}{Kuemmern Sie sich bitte selbst\MessageBreak um die Fontangelegenheiten}%
		   \else
			\ifthenelse{\equal{\TUBAF@hausschrift}{wasanners}}{}{%
				\ifthenelse{\equal{\TUBAF@hausschrift}{arial}}{%
					\IfFileExists{uarial.sty}{%
						\RequirePackage[scaled]{uarial}
						\renewcommand{\familydefault}{\sfdefault}
						\ifthenelse{\isundefined{\@pnumwidth}}{}{\renewcommand\@pnumwidth{1.85em}}% Breite für Seitenzahlen im toc
					   }{%
						\csname#1WarningNoLine\endcsname{\csname TUBAF#2@name\endcsname}{Kein Arial (Paket uarial) gefunden.\MessageBreak Gehe zur Grundeinstellung zurueck}
					}%
				   }{%
					\ifthenelse{\equal{\TUBAF@hausschrift}{times}}{%
						\IfFileExists{mathptmx.sty}{%
							\RequirePackage{mathptmx}
						   }{%
							\csname#1WarningNoLine\endcsname{\csname TUBAF#2@name\endcsname}{Kein Times (Paket mathptmx) gefunden.\MessageBreak Gehe zur Grundeinstellung zurueck}
						}
					   }{%
						\ifthenelse{\equal{\TUBAF@hausschrift}{false}}{%
							\ifthenelse{\equal{#3}{}}{}{\renewcommand{\familydefault}{#3}}%
						   }{%
							\RequirePackage[staerke=\TUBAF@hausschrift]{TUBAFhausschrift}
							\ifthenelse{\isundefined{\@pnumwidth}}{}{\renewcommand\@pnumwidth{1.85em}}% Breite für Seitenzahlen im toc
						}%
					}%
				}%
			}%
		\fi%
	\fi%
}
