% Sprachanpassungen
% \TUBAFSprachanpassung[Class/Package]{<Name ohne TUBAF>}
%
\newcommand{\TUBAFSprachanpassung}[2][Class]{%
	\ifthenelse{\equal{\TUBAF@sprache}{nobabel}}{%
		\AtBeginDocument{\@ifpackageloaded{babel}{\input{TUBAF#2.ldf}}{}}%
	   }{%
		\ifthenelse{\equal{\TUBAF@sprache}{ngerman}\OR\equal{\TUBAF@sprache}{german}}{%
			\RequirePackage[\TUBAF@addgreek english,\TUBAF@sprache]{babel}%
		   }{%
			\ifthenelse{\equal{\TUBAF@sprache}{english}\OR\equal{\TUBAF@sprache}{american}}{%
				\RequirePackage[\TUBAF@addgreek ngerman,\TUBAF@sprache]{babel}%
			   }{%
				\RequirePackage[\TUBAF@addgreek english,ngerman,\TUBAF@sprache]{babel}%
			}
		}%
		%
		\input{TUBAF#2.ldf}
		\PassOptionsToPackage{\TUBAF@sprache}{translator}% für siunitx u.a.
	}%
}
