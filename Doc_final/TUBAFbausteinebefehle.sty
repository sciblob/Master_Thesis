% TUBAFumrandung anstatt des frame-Befehls; arbeitet in allen Modi Anfang
%
\long\def\TUBAFUmrandung#1{%
  \leavevmode%
  \hbox{%
    \hskip-\@wholewidth%
    \vbox{%
      \vskip-\@wholewidth%
      \hrule \@height\@wholewidth%
      \hbox{%
        \vrule\@width\@wholewidth%
        #1%
        \vrule\@width\@wholewidth}%
      \hrule\@height\@wholewidth%
      \vskip-\@wholewidth}%
    \hskip-\@wholewidth}}
% wegen der Konsistenz
\newcommand{\TUBAFumrandung}[1]{%
	\@warning{Der Befehl 'TUBAFumrandung'\MessageBreak ist veraltet und kann in Zukunft geloescht sein.\MessageBreak Nutzen Sie bitte stattdessen 'TUBAFUmrandung'}%
	\TUBAFUmrandung{#1}%
}
