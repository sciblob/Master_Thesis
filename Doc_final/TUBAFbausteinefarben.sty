% Verschiebung der Farbhelligkeiten Anfang
% \TUBAFFarbe{<kompletter Farbname>}{<Starthelligkeit arabic>}{<Korrekturstufen +/- arabic>}
%
\newcommand{\TUBAFFarbe}[3]{%
	\begingroup
	\ifthenelse{\equal{#2}{}}{\setcounter{TUBAFtmpcnta}{0}}{\setcounter{TUBAFtmpcnta}{#2}}% leere Eingabe absichern
	\ifthenelse{\equal{#3}{}}{}{\addtocounter{TUBAFtmpcnta}{#3}}%
	\ifthenelse{\theTUBAFtmpcnta=0}{%
		\expandafter\xdef\csname#1ext\endcsname{}%
	   }{%
		\ifthenelse{\theTUBAFtmpcnta<0}{%
			\ifthenelse{\theTUBAFtmpcnta<-10}{%
				\PackageWarningNoLine{\TUBAFbausteine@name}{Sie sind jenseits der dunklen Seite des Mondes.\MessageBreak Die Helligkeit wird auf Minimum gesetzt}
				\expandafter\xdef\csname#1ext\endcsname{X}
			   }{%
				\setcounter{TUBAFtmpcntb}{\value{TUBAFtmpcnta} * -1}% Roman ! cnt > 0
				\expandafter\xdef\csname#1ext\endcsname{\Roman{TUBAFtmpcntb}}
			}%
		   }{%
			\ifthenelse{\theTUBAFtmpcnta>10}{%
				\PackageWarningNoLine{\TUBAFbausteine@name}{Allways look on the bright side of life!\MessageBreak Es ist jedoch zu hell.\MessageBreak Die Helligkeit wird auf Maximum gesetzt}
				\expandafter\xdef\csname#1ext\endcsname{x}
			   }{%
				\expandafter\xdef\csname#1ext\endcsname{\roman{TUBAFtmpcnta}}
			}
		}
	}
	\endgroup
}
