% Paketoptionen
\DeclareOptionX{farbschema}[rgb]{%
	\XKV@cc*+[\val\nr]{#1}{rgb,cmyk,gray}{%
	\ifcase\nr\relax
		\renewcommand{\TUBAFbausteinefarben@farbschema}{rgb}\or%
		\renewcommand{\TUBAFbausteinefarben@farbschema}{cmyk}\or%
		\renewcommand{\TUBAFbausteinefarben@farbschema}{gray}
	\fi    
	}{\PackageWarningNoLine{\TUBAFbausteinefarben@name}{Der Wert '\val' der Option 'farbschema'\MessageBreak ist falsch und wird ignoriert}}%
}
%
\DeclareOptionX{farbtafel}[k]{%
	\XKV@cc*+[\val\nr]{#1}{k,n}{%
	\ifcase\nr\relax
		\renewcommand{\TUBAFbausteinefarben@farbtafel}{k}\or%
		\renewcommand{\TUBAFbausteinefarben@farbtafel}{n}
	\fi    
	}{\PackageWarningNoLine{\TUBAFbausteinefarben@name}{Der Wert '\val' der Option 'farbtafel'\MessageBreak ist falsch und wird ignoriert}}%
}
%
\DeclareOptionX*{\PackageWarningNoLine{\TUBAFbausteinefarben@name}{Die unbekannte Option '\CurrentOption' wird ignoriert}}
%
\ExecuteOptionsX{%
	farbschema=rgb,
	farbtafel=k
}
\ProcessOptionsX\relax
